public class StringArrayTest {
    
    Public static List<String> generateStringArray(integer param)
    {
        List<String> stringArray= new List<string>();
        for(integer N=0; N<param; N++)
        {
            stringArray.add('Test '+N);
                        
        }
        
      //  System.debug(stringArray);
        return stringArray;
    }
        

}